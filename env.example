# AI Service API Keys
OPENAI_API_KEY=your_openai_api_key

# STT Service API Keys
SONIOX_API_KEY=your_soniox_api_key
SONIOX_MODEL=stt-rt-v3
DEEPGRAM_API_KEY=your_deepgram_api_key
SARVAM_API_KEY=your_sarvam_api_key

# TTS Service API Keys
CARTESIA_API_KEY=your_cartesia_api_key
ELEVEN_LABS_API_KEY=your_elevenlabs_api_key

# Service Provider Configuration
# STT Options: soniox, deepgram, sarvam
STT_PROVIDER=soniox

# TTS Options: cartesia, sarvam, murf
TTS_PROVIDER=cartesia

# Optional: Connect via Daily WebRTC locally
DAILY_API_KEY=your_daily_api_key


# To use Murf TTS, update your .env:

# Set TTS provider to murf
#TTS_PROVIDER=murf

MURF_API_KEY=your_murf_api_key_here

# Optional: Murf voice options
# en-US-ken (default male)
# en-US-natalie (female)
# en-US-wayne (deep male)
# en-IN-priya (Indian female)
# Check Murf docs for more voices